				<string># Get the latest release version 
# avoid using `jq`; it does not come with macOS out of the box
v=$(curl https://api.github.com/repos/mikyk10/alfred-keepass/releases/latest | grep '"tag_name": ' | grep -Eo 'v\d+\.\d+\.\d+'); v=${v#v}

latest=$v
current=$alfred_workflow_version

if [[ "ok" == "$(echo | awk "(${latest} &gt; ${current}) { print \"ok\"; }")" ]]; then
    echo -n ${latest}
elif [[ "ok" == "$(echo | awk "(${latest} == ${current}) { print \"ok\"; }")" ]]; then
    echo -n "up2date"
else
    echo -n "ahead"
fi</string>
